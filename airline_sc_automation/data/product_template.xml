        <record id="airline_sc_automation.ticket" model="product.template">
            <field name="name">Ticket</field>
            <field name="description">Airline Ticket</field>
            <field name="description_purchase">Airline Ticket</field>
            <field name="description_sale">Airline Ticket</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="standard_price">0.0</field>
            <field name="list_price">0.0</field>
            <field name="type">product</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="uom_po_id" ref="uom.product_uom_unit"/>
            <field name="image_1920" type="base64" file="airline_sc_automation/static/src/img/ticket.png"/>
            <field name="default_code">AIRLINE-TICKET</field>
